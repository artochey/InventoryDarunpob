@page
@model InventoryDarunpob.Pages.Siam.IndexSiamModel
@{
}
@{
	ViewData["Title"] = "Inventory System Home Page";
}
<a class="btn btn col-sm-3 alert-primary btn-sn" href="/Siam/CreateSiam">New item</a></a>
<table class="table">
	<thread>
		<tr>
			<th>Item ID</th>
			<th>Item </th>
			<th>Store ID</th>
			<th>Supplier ID</th>
			<th>Amount ID</th>
			<th>Created ID</th>
			<th>Action ID</th>
		</tr>
	</thread>
	<tbody>
		@foreach (var item in Model.listStock)
		{
			<tr>
				<td>@item.itemid</td>
				<td>@item.item</td>
				<td>@item.storeid</td>
				<td>@item.supplier</td>
				<td>@item.amount</td>
				<td>@item.create_at</td>
				<td>
					<a class="btn btn-primary btn-sm" href="/Siam/EditSiam?itemid=@item.itemid">Edit</a>
					<a class="btn btn-danger btn-sm" href="/Siam/DeleteSiam?itemid=@item.itemid">Delete</a>
				</td>
			</tr>

		}
	<tbody>
</table>
